<h1>New Post</h1>

<%= form_with url: cancancan_posts_path  do |form| %>
  <div>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :content %><br>
    <%= form.text_area :content %><br>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>

